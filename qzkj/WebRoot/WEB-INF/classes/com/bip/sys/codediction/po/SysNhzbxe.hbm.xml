        </id>
        <property name="nhzbbh" type="java.lang.String">
            <column name="NHZBBH" length="50" />
        </property>
        <property name="year" type="java.lang.String">
            <column name="YEAR" length="20" />
        </property>
        <property name="countryquota" type="java.lang.Double">
            <column name="COUNTRYQUOTA" precision="30" />
        </property>
        <property name="provincequota" type="java.lang.Double">
            <column name="PROVINCEQUOTA" precision="30" />
        </property>
    </class>
</hibernate-mapping>
