  <script type="text/javascript" src="sys/js/user_list.js"></script>
  <script type="text/javascript">
 function getres1()
 {
var deptno=$('#deptid').combotree('getValue');
if(deptno==null||deptno=="")
{
alert('请先选择所属部门！');
return;
}
  $('#res01').combobox({
    url:'$base/getResList.action?deptno='+deptno,
    valueField:'id',
    textField:'text',
    editable:false
});
}
function getSelTree2()
{
	$('#deptid').combotree(
	{
			url:'$base/getdepttree.action'
	}
	);
};
function getSelTree3()
{
	$('#regioncode').combotree(
	{
			url:'$base/getwfregiontree.action'
	}
	);
}
</script>	
</head>
<body class="easyui-layout">
 <div region="west" title="用户管理" split="true" style="width:480px;height:100%;padding1:1px;overflow:hidden;">
			<table id="tList">
           </table>
           <form id="mForm"  method="post" action="">
              <div >
                  <table width="100%" border="0" cellpadding="0" cellspacing="0" class="list">
				<tr>
				  <th>用户名：</th>
				  <td ><input id="username" name="username" type="text" class="input_text" size="20" maxlength="50"/>(<font 

class="hong">*</font>)
				      <input name="userid" id="userid" type="hidden" value=""/>
				</td>
