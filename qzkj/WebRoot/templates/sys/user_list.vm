			    <table width="90%" border="0" cellpadding="0" cellspacing="0"
						class="list">			     
			 
                  <tr>
                	  <th>角色名称：</th> 
                	   <td ><select id="roleid" name="roleid"  style="width:254px"> 
                             	<option value=""></option>
				    #foreach($temp in $session.getAttribute("rolelist"))
