			<div id="add_role" class="easyui-window" modal="true" title="添加" style="top:50px;padding: 10px;width:500px;height:350px;"
            iconCls="icon-add" closed="true" maximizable="false" minimizable="false" collapsible="false">
             <div id="aa_role">
